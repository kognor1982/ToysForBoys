          <ScalarProperty Name="Besteld" ColumnName="besteld" />
          <ScalarProperty Name="Prijs" ColumnName="prijs" />
          <ScalarProperty Name="CategorieID" ColumnName="categorieID" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>