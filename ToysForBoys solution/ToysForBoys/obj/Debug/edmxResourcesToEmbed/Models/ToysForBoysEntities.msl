        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Producten">
      <EntityTypeMapping TypeName="webshopModel.Product">
        <MappingFragment StoreEntitySet="producten">
          <ScalarProperty Name="ID" ColumnName="ID" />
